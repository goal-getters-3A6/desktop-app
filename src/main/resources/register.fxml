<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="registerpane" prefHeight="476.0" prefWidth="694.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controllers.RegisterController">
   <children>
      <TextField fx:id="prenomTxt" layoutX="33.0" layoutY="195.0" prefHeight="26.0" prefWidth="150.0" promptText="prenom" />
      <TextField fx:id="emailTxt" layoutX="33.0" layoutY="252.0" prefHeight="26.0" prefWidth="150.0" promptText="email" />
      <TextField fx:id="nomTxt" layoutX="33.0" layoutY="143.0" prefHeight="26.0" prefWidth="150.0" promptText="nom" />
      <PasswordField fx:id="mdpRegisterTxt" layoutX="33.0" layoutY="333.0" prefHeight="26.0" prefWidth="150.0" promptText="Mot de passe" />
      <Button fx:id="mdpRegisterBtn" layoutX="211.0" layoutY="404.0" mnemonicParsing="false" onAction="#register" text="Registrer" />
      <DatePicker fx:id="dateNaissance" layoutX="245.0" layoutY="211.0" promptText="date_naissance" />
      <Button fx:id="uploadPicBtn" layoutX="287.0" layoutY="266.0" mnemonicParsing="false" onAction="#importProfilePic" text="Upload photo" />
      <TextField fx:id="telTxt" layoutX="34.0" layoutY="296.0" prefHeight="26.0" prefWidth="150.0" promptText="Tel" />
   </children>
</AnchorPane>
