                <Font name="System Bold" size="14.0" />
            </font></TextField>
        <Label layoutX="299.0" layoutY="1.0" text="Admin Dashboard" textFill="WHITE">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <BarChart fx:id="barChart" barGap="1.0" categoryGap="1.0" layoutX="201.0" layoutY="58.0" prefHeight="218.0" prefWidth="584.0" style="-fx-background-color: white;" title="Users birthday month chart">
            <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" upperBound="40.0" />
            </yAxis>
        </BarChart>
      <ListView fx:id="userslistview" layoutX="29.0" layoutY="361.0" prefHeight="178.0" prefWidth="749.0" />
      
    </children>
